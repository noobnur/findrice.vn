<!DOCTYPE html>
<html>
  <head>
    <style>
      /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
      #map {
        height: 600px;
		width: 650px;
		position: absolute;
		left: 200px;
		top: 50px;
      }
      /* Optional: Makes the sample page fill the window. */
      /*html, body {
        height: 100%;
        margin: 0;
        padding: 0;*/
      }
    </style>
  </head>
  <body>
	<div>
		<h1>Prospects</h1>
	</div>
    <div id="map"></div>
    <script>
		var map;
		function initMap() {
			map = new google.maps.Map(document.getElementById('map'), {
			  zoom: 6,
			  center: new google.maps.LatLng(18.782292, 105.885979),
			  mapTypeId: 'roadmap'
			});

			// City 1 : Vinh
			var contentString = '<div id="content">'+
			  '<div id="siteNotice">'+
			  '</div>'+
			  '<h1 id="firstHeading" class="firstHeading">Vinh</h1>'+
			  '<div id="bodyContent">'+
			  '<p><b>Vinh</b> is the biggest city and economic and cultural center of the North Central Coast of Vietnam.</br>' +
			  'It is a key point in the East–West economic corridor linking Myanmar, Thailand, Laos and Vietnam.</br>' +
			  'It has a population size of 490,000<p>'+
			  '</div>'+
			  '</div>';

			var infowindow = new google.maps.InfoWindow({
				content: contentString
			  });

			var marker = new google.maps.Marker({
				position: {lat: 18.679585, lng: 105.681335},
				map: map,
				title: 'Vinh'
			});
			marker.addListener('click', function() {
				infowindow.open(map, marker);
			});

			// City 2 : Lào Cai
			var contentString2 = '<div id="content">'+
			  '<div id="siteNotice">'+
			  '</div>'+
			  '<h1 id="firstHeading" class="firstHeading">Lào Cai</h1>'+
			  '<div id="bodyContent">'+
			  '<p><b>Lào Cai</b> is a city in the Northwest region of Vietnam.</br>' +
			  'It has a population size of 98,363</br><p> '+
			  '</div>'+
			  '</div>';

			var infowindow2 = new google.maps.InfoWindow({
				content: contentString2
			  });

			var marker2 = new google.maps.Marker({
				position: {lat: 22.338155, lng: 104.148755},
				map: map,
				title: 'Laocai'
			});
			marker2.addListener('click', function() {
				infowindow2.open(map, marker2);
			});

			// City 3 : Phủ Lý
			var contentString3 = '<div id="content">'+
			  '<div id="siteNotice">'+
			  '</div>'+
			  '<h1 id="firstHeading" class="firstHeading">Phủ Lý</h1>'+
			  '<div id="bodyContent">'+
			  '<p><b>Phủ Lý</b>is the capital city of Hà Nam Province of Vietnam 60 km south of Hanoi on the river Đáy</br>' +
			  'sandstone rock formation in the southern part of the '+
			  'Northern Territory, central Australia. It lies 335&#160;km (208&#160;mi) '+
			  '(last visited June 22, 2009).</p>'+
			  '</div>'+
			  '</div>';

			var infowindow3 = new google.maps.InfoWindow({
				content: contentString3
			  });

			var marker3 = new google.maps.Marker({
				position: {lat: 20.531755, lng: 105.917655},
				map: map,
				title: 'Vinh'
			});
			marker3.addListener('click', function() {
				infowindow3.open(map, marker3);
			});

		}
    </script>
		<script async defer
                src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBMm6zu1aVttw8chkv3b4ZqKBQuCYRimAs&callback=initMap">
        </script>
	 </body>
</html>
